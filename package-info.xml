	<author>HippoEDIT.com</author>
	<email>supportbox@hippoedit.com</email>
	<homepage>http://wiki.hippoedit.com/plugins/jsminifier</homepage>
	<memo></memo>
	<install>
		<readme type="inline">This will install JSMinifier plugin for HippoEDIT.</readme>
		<require-file name="main.hejs" destination="%{plugins}%/jsminifier/main.hejs" />
